	url = {https://www.tandfonline.com/doi/full/10.1080/00222895.2021.2001412},
	doi = {10.1080/00222895.2021.2001412},
	abstract = {This study examined the influence of muscle activation and motor unit number estimation ({MUNE}) on early phase voluntary rate of torque development ({RTD}) in younger ({YM}) and older ({OM}) men. Thirty-two {YM} (n ¼ 17; Age ¼ 22 yrs) and {OM} (n ¼ 15; Age ¼ 74 yrs) volunteered to participate in this study. Early phase {RTD} (first 50 ms of a rapid isometric contraction; {RTD}50) and normalized surface electromyography (first 50 ms of muscle excitation; {nEMG}50) were recorded from the right quadricep muscle group. {MUNE} was examined from the right vastus lateralis. Multiple linear regression analyses revealed that {nEMG}50 had a significant effect on {RTD}50 independent of age group (p 0.001). {nEMG}50 had a significant effect on {RTD}50 in the {OM} group (p ¼ 0.037). {MUNE} had no effect on {RTD}50 independent of age. Older adults may depend more on muscle activation at contraction onset of early phase {RTD} compared to younger adults.},
	pages = {1--7},
	journaltitle = {Journal of Motor Behavior},
	shortjournal = {Journal of Motor Behavior},
	author = {Magrini, Mitchel A. and Colquhoun, Ryan J. and Ferrell, Matt C. and Fleming, Sydnie R. and Mota, Jacob A. and Siedlik, Jacob A. and Poidomani, Nick M. and Jenkins, Nathaniel D.M and {DeFreitas}, Jason M.},
