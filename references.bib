	keywords = {Animals, Calcium Channels, L-Type/physiology, Muscle Contraction/*physiology, Muscle, Skeletal/physiology, Ryanodine Receptor Calcium Release Channel/physiology, Sarcoplasmic Reticulum/*physiology},
}

